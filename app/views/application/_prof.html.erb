<div class="sub">
	<h1>profile</h1>
	<%= image_tag "/docs/#{mem.id.to_s}-#{mem.created_at.to_s}-#{mem.photo}",
	  alt: 'image', width: 300, height: 400 %>
	<!-- <img src="" width="200" height="250" alt="photo"> -->
	<dl class="profile">
		<dt>Auther:</dt>
		<dd><%= mem.name %></dd>
		<dt>Birthday:</dt>
		<dd><%= mem.birthday %></dd>
		<dt>From:</dt>
		<dd><%= mem.comefrom %></dd>
		<dt>interest:</dt>
		<dd><%= mem.interest %></dd>
	</dl>
		
	<% if @other.nil? %>
		<p><%= link_to 'プロフィール変更', edit_member_path(mem) %></p>
	<% end %>
</div><!-- .sub -->
			
<div class="sub">
	<h1>the latest article</h1>
	<% @articles5.each do |article| %>
    	<p><%= link_to article.title + '(' + article.created_at.strftime('%Y/%m/%d') + ')', article %></p>
	<% end %>
</div><!-- sub -->

